package com.bj04.While;

import java.util.Scanner;

public class Main03 {
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int num = sc.nextInt();
		
		int num1 = 0;			// 십의자리 숫자
		int num2 = 0;			// 일의자리 숫자
		
		int cnt = 0;			// 사이클
		int res = num;		//최초 입력숫자 저장하는 변수
		int tmp = 0;			// 일의자리 + 십의자리
		
		do {
			
			num1 = num / 10;
			num2 = num % 10;
			
			tmp = num1 + num2;
			
			if(tmp>=10) {
				tmp = tmp % 10;
			}
			
			num = (num2 * 10) + tmp;
			cnt++;
		}while(num != res);
		
		System.out.println(cnt);
	}

}